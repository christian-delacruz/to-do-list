<script>
    function toggleStrikethrough(checkbox) {
        var taskContent = checkbox.nextElementSibling;
        if (checkbox.checked) {
            taskContent.classList.add('strikethrough');
        } else {
            taskContent.classList.remove('strikethrough');
        }
    }
</script>
